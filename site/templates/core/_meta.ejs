<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta rel="canonical" href="<%= meta.root %>">

<title><%= meta.title %></title>
<meta name="description" content="<%= meta.description %>">
<meta name="keywords" content="<%= meta.keywords %>">
<meta name="author" content="Ted Noelker">
<meta name="revised" content="<%= new Date().toLocaleDateString('en-US') %>">

<meta name="og:title" content="TED NOELKER">
<meta name="og:description" content="<%= meta.description %>">
<meta name="og:image" content="<%= meta.root + meta.path %>/preview.jpg">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:creator" content="<%= meta.twitter %>">
<meta name="theme-color" content="<%= meta.color %>">
<meta name="msapplication-TileColor" content="<%= meta.color %>">
<meta name="msapplication-config" content="<%= meta.root + meta.path %>/browserconfig.xml">
<meta name="google-site-verification" content="xcz5-YGtzOtgaR07R8_2C9Tx2viyrdYh9wpkf-FxD5g" />

<link rel="apple-touch-icon" sizes="180x180" href="<%= meta.root + meta.path %>/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="<%= meta.root + meta.path %>/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="<%= meta.root + meta.path %>/favicon-16x16.png">
<link rel="shortcut icon" href="<%= meta.root + meta.path %>/favicon.ico">
<link rel="manifest" href="<%= meta.root + meta.path %>/site.webmanifest">
<link rel="mask-icon" href="<%= meta.root + meta.path %>/safari-pinned-tab.svg" color="<%= meta.color %>">

<style>body { opacity: 0; }</style>
<link rel="preload" href="https://fonts.googleapis.com/css?family=Montserrat:800" as="style" onload="this.rel='stylesheet'" crossorigin>
<link rel="preload" href="/assets/css/global.css" as="style" onload="this.rel='stylesheet'">
